#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[100][100],i,j,sum = 0;
    for( i=0;i<n;i++){
        for( j = 0; j<n;j++){
            scanf("%d",&a[i][j]);
            if(i == 0|| i == n-1||j == 0|| j == n-1)
            sum += a[i][j];
        }
       
    }
    
   printf("%d\n",sum);
    return 0;
}
